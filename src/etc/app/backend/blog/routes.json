[
	{
		"url":"/blog/posts/new",
		"action":"insertPost"
	},
	{
		"url":"/blog/posts/([^/]+)/update",
		"action":"updatePost",
		"vars": ["postName"]
	},
	{
		"url":"/blog/posts/([^/]+)/delete",
		"action":"deletePost",
		"vars": ["postName"]
	},
	{
		"url":"/blog/posts/([^/]+)/comments",
		"action":"listPostComments",
		"vars": ["postName"]
	},
	{
		"url":"/blog/comments/([0-9]+)/update",
		"action":"updateComment",
		"vars": ["commentId"]
	},
	{
		"url":"/blog/comments/([0-9]+)/delete",
		"action":"deleteComment",
		"vars": ["commentId"]
	},
	{
		"url":"/blog/config",
		"action":"updateConfig"
	}
]