[
	{
		"url":"/blog/?",
		"action":"index"
	},
	{
		"url":"/blog/rss\\.xml",
		"action":"showRssFeed"
	},
	{
		"url":"/blog/atom\\.xml",
		"action":"showAtomFeed"
	},
	{
		"url":"/blog/page/([0-9]+)/?",
		"action":"showPage",
		"vars": ["pageNbr"]
	},
	{
		"url":"/blog/post/([a-zA-Z0-9-_.]+)/?",
		"action":"showPost",
		"vars": ["postName"]
	},
	{
		"url":"/blog/tag/([a-zA-Z0-9-_.]+)/?",
		"action":"showTag",
		"vars": ["tagName"]
	}
]